    "paths": {
        "/follow": {
            "post": {
                "description": "Create a follow relationship of two users",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Create a follow relationship",
                "parameters": [
                    {
                        "description": "JSON describing relationship",
                        "name": "follow",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.Follow"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/model.Follow"
                        }
                    },
                    "409": {
                        "description": "Conflict",
                        "schema": {
                            "$ref": "#/definitions/utils.HttpError"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "model.Follow": {
            "type": "object",
            "properties": {
                "followee": {
                    "type": "string"
                },
                "follower": {
                    "type": "string"
                }
            }
        },
        "utils.HttpError": {
            "type": "object",
            "properties": {
                "Err": {
                    "type": "string"
                }
            }
        }
    }
